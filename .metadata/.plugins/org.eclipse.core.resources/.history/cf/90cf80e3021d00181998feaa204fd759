package textExcel;

public class RealCell implements Cell {

	public String num;
	private Location loc;
	
	public RealCell(String text, Location loc) {
		this.num = text;
		this.loc = loc;
	}
		
	@Override
	public String abbreviatedCellText() {
		return String.format("%-10.10s", num);
	}

	@Override
	public String fullCellText() {
		return String.format("%s", getDoubleValue(num));
	}
	
	public double getDoubleValue(String numb) {
		return Double.parseDouble(numb);
	}
}
