package textExcel;

public class TextCell implements Cell{

	private String text;
	
	public TextCell(String text) {
		this.text = text;
	}
	
	@Override
	public String abbreviatedCellText() {
		return String.format("|%.10s|", text);
	}

	@Override
	public String fullCellText() {
		return String.format("|%s|", text);
	}

}
