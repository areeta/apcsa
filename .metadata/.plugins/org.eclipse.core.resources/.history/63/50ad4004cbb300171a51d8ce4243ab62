import java.util.Arrays;

public class CopiesEverywhere {

	public static void main(String[] args) {
		int[] a = {1, 2, 3};
		int[] b = a;
		int[] c = new int[3];
		
		for (int i = 0; i < a.length; i++) {
			c[i] = a[i];
		}
		
		System.out.println(Arrays.toString(a));
		System.out.println(a);
	}
}
