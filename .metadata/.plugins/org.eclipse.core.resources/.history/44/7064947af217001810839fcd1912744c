
public class StackTracingClient {
	
	public static void main(String[] args) {
		
		/* * * * * * * * * * STACKS AND DEBUGGING * * * * * * * * * * * * * * * */
		// First stack tracing problem
		int answer = 0;
		answer = StackTracing.traceMe(answer);
		System.out.print(answer);
		

		// Second stack tracing problem
		int answer2 = 0;
		answer2 = StackTracing2.traceThis(answer2);
		System.out.print(answer2);
		
	}
}
